@model Kentico.Kontent.Delivery.Abstractions.IContentType

@{
    var elementValues =  Model.Elements.Select(e => e.Value);
}

<h2>@Model.System.Name</h2>

<ul class="list-group list-group-flush">
    <li class="list-group-item"><label>Codename:&nbsp;</label><code>@Model.System.Codename</code></li>
    <li class="list-group-item"><label>ID:&nbsp;</label><code>@Model.System.Id</code></li>
    <li class="list-group-item"><label>Last Modified:&nbsp;</label><code>@Model.System.LastModified</code></li>
</ul>




<table class="table table-striped table-sm">
    <tr class="thead-light d-flex">
        <th class="col col-4">Name</th>
        <th class="col col-4">Codename</th>
        <th class="col col-4">Type</th>
    </tr>
    @Html.DisplayFor(m => elementValues)
</table>
